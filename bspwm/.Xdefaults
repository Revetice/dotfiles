
Xft.dpi:                    96
! Xft.antialias:              false
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

! Tango colour palette
*foreground: white
*background: black

*color0:  #2E3436
*color1:  #a40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #ce5c00
*color7:  #babdb9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #fcaf3e
*color15: #EEEEEC

! URxvt
URxvt.geometry:             90x30
URxvt.transparent:          false
! URxvt.transparent:          true
URxvt.fading:               0
! URxvt.urgentOnBell:         true
! URxvt.visualBell:           true
URxvt.loginShell:           true
URxvt.saveLines:            50
URxvt.internalBorder:       3
URxvt.lineSpace:            0

URxvt.allow_bold:           true
! URxvt.font:                 -*-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-1
URxvt*font:                 xft:Hack:pixelsize=14
URxvt*letterSpace:          -1

URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false

URxvt.pointerBlank:         true

URxvt*depth:                32
URxvt*background:			[80]#000000
! URxvt.background:			rgba:0000/0000/0000/ffff

! Clickable URL
URxvt.perl-ext-common:		default,matcher
URxvt.url-launcher: 		/usr/bin/google-chrome-stable
URxvt.matcher.button:		1

! Dmenu
Dmenu*font:                 xft:Hack:pixelsize=14

